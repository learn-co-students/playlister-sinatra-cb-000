<h1><%= @genre.name %> GENRE</h1>
<h3>Songs:</h3>
<ul>
  <% @genre.songs.each{|song| %>
    <li><a href='/songs/<%=song.slug%>'><%= song.name %></a></li>
  <% } %>
</ul>
<h3>Artists:</h3>
<ul>
  <% @genre.artists.each{|artist| %>
    <li><a href='/artists/<%=artist.slug%>'><%= artist.name %></a></li>
  <% } %>
</ul>
