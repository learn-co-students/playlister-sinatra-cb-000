<h2><%= @artist.name %></h2>
<h3>Songs:</h3>
<ul>
  <% @artist.songs.each{|song| %>
    <li><a href='/songs/<%=song.slug%>'><%= song.name %></a></li>
  <% } %>
</ul>
<h3>Genres:</h3>
<ul>
  <% @artist.genres.each{|genre| %>
    <li><a href='/genres/<%=genre.slug%>'><%= genre.name %></a></li>
  <% } %>
</ul>
