<H1> Artist Details</h1>
  <p>Artist Name: <%= @artist.name %></p>

  <% @artist.songs.each do |song| %>
    <p>Song Title: <a href="/songs/<%= song.slug %>"><%= song.name %></a></p>
  <% end %>

  <% @artist.genres.each do |genre| %>
    <p>Genre: <a href="/genres/<%= genre.slug %>"><%= genre.name %></a></p>
  <% end %>
