<h1>New Song</h1>
<form action="/songs" method="post">
  <p>Song Name: <input type="text" name="song[name]" id="Name"></input></p>
  <p>Artist Name: <input type="text" name="artist_name" id="Artist Name"></input></p>
  <p>
    <% @genres.each do |genre| %>
      <input type="checkbox" name="genres[]" value="<%= genre.id %>" id="<%= genre.name %>"><%= genre.name %></input>
    <% end %>
  </p>
  <p><input type="submit" value="Create"></p>
</form>
