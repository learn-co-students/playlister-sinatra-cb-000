<form method="post" action="/songs">
  <label>Song Name: </label>
  <input type="text" name="Name" id="song_name">
  <br>
  <br>

  <label>Genre Selection: </label>
  <br>
  <% Genre.all.each do |genre| %>
    <input type="checkbox" name="Genre[]" value="<%= genre.name %>" id="<%= genre.name %>"><%= genre.name %>
    <br>
  <% end %>
  <br>

  <label>Artist Name: </label>
  <input type="text" name="Artist Name" id="artist_id">

  <input type="submit" id="Create">
</form>