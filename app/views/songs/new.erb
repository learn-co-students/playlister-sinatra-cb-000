<form action="/songs" method="post">
  <label>Song Name:</label>
  <input type="text" name="name" id="Name">
  <br>
  <label>Artist Name:</label>
  <input type="text" name="artist[name]" id="Artist Name">
  <br>
  <label>Genre:</label>
  <%Genre.all.each do |g|%>
  <label><%g.name%></label>
  <input type="checkbox" name="genres[]" id="<%=g.name%>" value="<%=g.id%>"><%=g.name%></input>
  <%end%>
  <input type="submit" name="create" id="Create">
</form>
