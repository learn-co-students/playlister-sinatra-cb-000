<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1><%= @song.name %></h1>
<h3>Artist: <%= @song.artist.name %> <a href="/artists/<%= @song.artist.slug %>">link</a></h3>
<h3>Genres:</h3>
<% @song.genres.each do |genre| %>
<p><%= genre.name %> <a href="/genres/<%= genre.slug %>">link</a></p>
<% end %>
