<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1>Song Title: <%= @song.name %></h1>

<h3>Artist Name: <a href="/artists/<%= @song.artist.slug %>"><%= @song.artist.name %></a></h3>
<h3>Genres: <% @song.genres.each do |genre| %>
              <a href="/genres/<%= genre.slug %>"><%= genre.name %></a>
            <% end %></h3>
