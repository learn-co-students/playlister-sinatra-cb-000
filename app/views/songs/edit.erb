<form method="post" action="/songs/<%=@song.slug%>">
<input type="hidden" id="hidden" name="_method" value="patch">

<input type="text" name="name" id="song_name" value="<%=@song.name%>">

<input type="text" name="artist" id="Artist Name" value="<%=@song.artist.name%>">

<% Genre.all.each do |genre| %>
  <input type="checkbox" name="genres[]" value="<%=genre.id%>" id="<%=genre.name%>"><%=genre.name%></input>
<% end %>

<input type="submit" value="Save" id="Save">
</form>